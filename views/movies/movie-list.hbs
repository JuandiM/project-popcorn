<h1>movies</h1>


<ul>
    {{#each movies}}
        <li>
            <h4><a href="/movies/movie-detail/{{this.id}}">{{this.original_title}}</a></h4>
            <p>{{this.id}}</p>
            <img src="https://image.tmdb.org/t/p/original/{{this.poster_path}}" alt="" width="200px">
            <p>{{this.release_date}}</p>
        </li>
    {{/each}}
</ul>
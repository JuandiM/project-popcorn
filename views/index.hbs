
<h1>upcomingMovies</h1>
<section class="section">
  <div id="carousel-upcoming">
    
    {{#each upcomingMovies}}
        <div class="item">
            <a href="/movies/movie-detail/{{this.id}}">
                <div class="card-image">
                        <img src="https://image.tmdb.org/t/p/original/{{this.poster_path}}" alt="movie poster to {{this.original_title}}">
                </div>
            </a>
        </div>
    {{/each}}

  </div>
</section>
